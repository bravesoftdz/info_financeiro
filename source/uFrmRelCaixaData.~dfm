  object qryEntradaSaida: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'pMODALIDADE'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftInteger
        Name = 'pSEQUENCIA'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftInteger
        Name = 'pSEQFINAL'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftInteger
        Name = 'pCAIXA'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftString
        Name = 'pTIPOMOV'
        ParamType = ptInput
        Value = ''
      end>
    SQL.Strings = (
      'Select MCA_MODALIDADE, MCA_NATUREZA, MCA_OBSEVACAO,'
      'MCA_VLMODALIDADE from movimento_caixa '
      'Where (MCA_MODALIDADE = :pMODALIDADE)'
      'AND (MCA_SEQUENCIA >= :pSEQUENCIA) '
      'AND (MCA_SEQUENCIA <= :pSEQFINAL) '
      'AND (MCA_CAIXA = :pCAIXA) '
      'AND (MCA_TIPOMOV = :pTIPOMOV)')
    SQLConnection = Dm.SqlAdmin
    Left = 88
    Top = 96
  end
