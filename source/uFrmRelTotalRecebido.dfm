  object dstDados: TSQLDataSet
    CommandText = 
      'Select r.rec_loja, Sum(R.rec_valorparcela) AS ARECEBER, SUM(R.re' +
      'c_pago) AS RECEBIDO from CONTASRECEBER R Where (extract(year fro' +
      'm R.REC_DATAVENCIMENTO) = :pANO) AND (extract(MONTH from R.REC_D' +
      'ATAVENCIMENTO) = :pMES) and (REC_LOJA = :pLOJA) Group by r.rec_l' +
      'oja'#13#10
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'pANO'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'pMES'
        ParamType = ptInput
      end
      item
        DataType = ftInteger
        Name = 'pLOJA'
        ParamType = ptInput
      end>
    SQLConnection = Dm.SqlAdmin
    Left = 32
