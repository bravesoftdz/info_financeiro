  object dstContasReceber: TSQLDataSet
    CommandText = 
      'Select REC_SEQUENCIA, REC_LOJA, REC_CREDITO, REC_PARCELA, REC_QT' +
      'PARCELAS, REC_CLIENTE, REC_NRVENDA, REC_DATAEMISSAO, REC_DATAVEN' +
      'CIMENTO, REC_VALORPARCELA, REC_PAGO, REC_SITUACAO, REC_DATAPAGAM' +
      'ENTO, REC_FLBOLETO, REC_NOSSO_NUMERO, REC_VLMULTA, REC_SEUNUMERO' +
      ', REC_VALORJUROS, REC_VALORDESCONTO, REC_NRFATURA, REC_DESCRICAO' +
      ', CL.cli_cdgrupo from CONTASRECEBER Inner Join CLIENTES CL on CL' +
      '.cli_codigo = CONTASRECEBER.rec_cliente where (REC_DATAVENCIMENT' +
      'O >= :pDTINICIO) AND (REC_DATAVENCIMENTO <= :pDTFINAL)'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftDateTime
        Name = 'pDTINICIO'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'pDTFINAL'
        ParamType = ptInput
      end>
    SQLConnection = Dm.SqlAdmin
    Left = 344
    Top = 216
  end
