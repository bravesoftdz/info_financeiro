  object dstTransferencias: TSQLDataSet
    CommandText = 
      'Select T.TRA_DATA,'#13#10'(Select L.EMP_FANTASIA from EMPRESAS L Where' +
      ' (L.EMP_CODIGO = T.TRA_LOJAORIGEM)) AS LOJ_ORIGEM,'#13#10'(Select L.EM' +
      'P_FANTASIA from EMPRESAS L Where (L.EMP_CODIGO = T.TRA_LOJADESTI' +
      'NO)) AS LOJ_DESTINO,'#13#10'H.HIS_PRODUTO, P.PRO_DESCRICAO, P.UNI_CODI' +
      'GO, H.HIS_MOVIMENTO,'#13#10'H.HIS_VLCOMPRA'#13#10'from HISTORICO H'#13#10'INNER jo' +
      'in PRODUTOS P ON P.PRO_CODIGO = H.HIS_PRODUTO'#13#10'INNER join TRANSF' +
      'ERENCIAS T ON T.tra_nrtransferencia = H.HIS_DOCUMENTO'#13#10'Where (H.' +
      'HIS_TIPO = '#39'TS'#39')'#13#10'and (H.HIS_DATA >= :pDTINICIAL) AND (H.HIS_DAT' +
      'A <= :pDTFINAL)'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftDate
        Name = 'pDTINICIAL'
        ParamType = ptInput
        Value = 0d
      end
      item
        DataType = ftDate
        Name = 'pDTFINAL'
        ParamType = ptInput
        Value = 0d
      end>
    SQLConnection = Dm.SqlAdmin
    Left = 8
    Top = 184
  end
