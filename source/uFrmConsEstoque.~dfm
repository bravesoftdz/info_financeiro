  object dstConsEstoque: TSQLDataSet
    CommandText = 
      'Select E.EST_CODIGOLOJA, E.PRO_CODIGO, P.PRO_DESCRICAO, E.EST_QU' +
      'ANTIDADE'#13#10'from ESTOQUE E'#13#10'INNER join PRODUTOS P ON P.PRO_CODIGO ' +
      '= E.PRO_CODIGO'#13#10'Where (E.EST_CODIGOLOJA = :pCDLOJA) '#13#10'order by E' +
      '.PRO_CODIGO '
    MaxBlobSize = -1
    Params = <
