  object dstContasReceber: TSQLDataSet
    CommandText = 
      'Select * from CONTASRECEBER Where (REC_LOJA = :pLOJA)'#13#10'AND (REC_' +
      'CREDITO = :pNRCREDITO) '#13#10'AND (REC_PARCELA = :pPARCELA)'#13#10#13#10
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
        Name = 'pLOJA'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftInteger
        Name = 'pNRCREDITO'
        ParamType = ptInput
        Value = 0
      end
      item
        DataType = ftString
        Name = 'pPARCELA'
        ParamType = ptInput
        Value = ''
      end>
    SQLConnection = Dm.SqlAdmin
    Left = 184
    Top = 32
