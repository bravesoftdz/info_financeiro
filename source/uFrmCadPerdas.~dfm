  object qryProdutos: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftString
        Name = 'pCODIGO'
        ParamType = ptInput
        Value = ''
      end>
    SQL.Strings = (
      'Select PRO_CODIGO, PRO_VLCOMPRA, PRO_VLCUSTO, PRO_VLVENDA '
      'from PRODUTOS Where (PRO_CODIGO = :pCODIGO)')
    SQLConnection = Dm.SqlAdmin
    Left = 448
    Top = 200
  end
  object qryTotVendido: TSQLQuery
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftString
        Name = 'pCODIGO'
        ParamType = ptInput
        Value = ''
      end
      item
        DataType = ftDate
        Name = 'pDATA'
        ParamType = ptInput
        Value = 0d
      end
      item
        DataType = ftString
        Name = 'pTIPO'
        ParamType = ptInput
        Value = ''
      end>
    SQL.Strings = (
      'Select Sum(HIS_MOVIMENTO) as TOTAL from HISTORICO '
      'Where (HIS_CODIGOBAIXA = :pCODIGO) AND'
      '(HIS_DATA = :pDATA) and '
      '(HIS_TIPO = :pTIPO )')
    SQLConnection = Dm.SqlAdmin
    Left = 484
    Top = 198
    object qryTotVendidoTOTAL: TFMTBCDField
      FieldName = 'TOTAL'
      Precision = 15
      Size = 3
    end
  end
